    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/tsconfig": "^0.1.3",
    "openapi-typescript-codegen": "^0.20.1",
    "typescript": "~4.5.5",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.8.4",
